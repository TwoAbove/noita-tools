  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "build-noita_random": "pwsh -nol -noni -nop -f src/services/SeedInfo/noita_random/build.ps1",
    "build": "tsc && vite build",
    "console-build": "npx rimraf console-build/* console-build/* && node consoleBuild.cjs",
    "console-search": "node --no-warnings=ExperimentalWarning console-build/consoleSearch.js",
    "cy": "cypress open --e2e",
    "dev-win": "concurrently --restart-tries -1 \"nodemon --experimental-modules ./server/index.mjs\" \"vite\"",
    "dev": "NODE_ENV=development concurrently --restart-tries -1 'nodemon --experimental-modules ./server/index.mjs' 'vite' 'bash watch_build.sh'",
    "e2e": "npm run build && concurrently -k  -s first 'cypress run' 'PORT=3000 npm run start'",
    "prepare": "husky install",
    "start": "node --experimental-modules ./server/index.mjs",
    "test": "vitest"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "nodemonConfig": {
    "watch": [
      "server"
    ],
    "ignore": [
      "fontGenerator/*",
      "public/*",
      "src/*"
    ]
  },

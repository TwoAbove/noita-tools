  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "build-noita_random": "pwsh -nol -noni -nop -f src/services/SeedInfo/noita_random/build.ps1",
    "build": "craco build",
    "console-build": "npx rimraf console-build/* console-build/* && node consoleBuild.js",
    "console-search": "node --no-warnings=ExperimentalWarning console-build/consoleSearch.js",
    "cy": "cypress open --e2e",
    "dev-win": "concurrently --restart-tries -1 \"nodemon ./server\" \"craco start\"",
    "dev": "NODE_ENV=development concurrently --restart-tries -1 'nodemon ./server' 'craco start > /dev/null' 'cd src/services/SeedInfo/noita_random && while inotifywait -e close_write **/** **; do bash build.sh; done;' 'cd src/services/SeedInfo/infoHandler && while inotifywait -e close_write **/** **; do bash build.sh; done;'",
    "e2e": "npm run build && concurrently -k  -s first 'cypress run' 'PORT=3000 npm run start'",
    "prepare": "husky install",
    "start": "node ./server",
    "test": "craco test"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "nodemonConfig": {
    "watch": [
      "server"
    ],
    "ignore": [
      "fontGenerator/*",
      "public/*",
      "src/*"
    ]
  },
  "browserslist": {
    "production": [
      "supports wasm",
      "not dead"
